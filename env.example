# TMDB API Configuration
# Get your API key from https://www.themoviedb.org/settings/api
VITE_TMDB_API_KEY=your_tmdb_api_key_here

# Google Analytics (Optional)
VITE_GA_TRACKING_ID=your_google_analytics_id_here

# PropellerAds Configuration
# Get your publisher ID and ad unit IDs from https://propellerads.com
VITE_PUBLISHER_ID=your_propellerads_publisher_id_here
VITE_BANNER_ABOUT_AD_UNIT_ID=your_banner_ad_unit_id_for_about_section
VITE_BANNER_MOVIE_CARD_AD_UNIT_ID=your_banner_ad_unit_id_for_movie_card
VITE_INTERSTITIAL_AD_UNIT_ID=your_interstitial_ad_unit_id

# Development Configuration
NODE_ENV=development

# CORS Server Configuration (for development)
CORS_PORT=3001 